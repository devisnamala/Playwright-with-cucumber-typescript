{
  "name": "humana",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "pretest": "npx ts-node src/utils/init.ts",
    "test": "cucumber-js --require-module ts-node/register --require src/hooks/hooks.ts --require src/test/steps/*.ts --format json:test-results/cucumber.json",
    "testtag": "cucumber-js",
    "report": "ts-node /Users/siri/Desktop/Humana/src/utils/report.ts && open test-results/reports/index.html",
    "test:record": "npx playwright codegen",
    "htmlreport": "open test-results/reports/cucumber_report.html",
    "test:failed": "cucumber-js -p rerun @rerun.txt"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@playwright/test": "^1.44.1",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.14.2",
    "cucumber-html-reporter": "^7.1.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "ajv": "^8.16.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^9.0.0",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "tsc": "^2.0.4",
    "winston": "^3.13.0"
  }
}
